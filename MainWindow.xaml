<Window x:Class="_15._03._2023.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_15._03._2023"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label Content="Username"/>
            <TextBox x:Name="username"></TextBox>
            <Label Content="password"/>
            <PasswordBox x:Name="password"/>
            <Button x:Name="registration" Content="Регистрация" Click="registration_Click"></Button>
            <Button x:Name="authorization" Content="Авторизация" Click="authorization_Click"></Button>
        </StackPanel>
    </Grid>
</Window>
